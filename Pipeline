pipeline{
    agent any

    stages{
        stage("Extract Data"){
            steps{
                sh 'python3 data_extraction.py'
            }
        }
        stage("Data Analysis"){
            steps{
                sh 'python3 data_analysis.py'
            }
        }
        stage("Data Preprocessing"){
            steps{
                sh 'python3 data_preprocessing.py'
            }
        }

        }
}
